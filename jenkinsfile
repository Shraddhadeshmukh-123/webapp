pipeline {
    agent any

    stages {
        stage('git') {
            steps {
                echo 'Hello World'
                git credentialsId: 'jenkins-key', url: 'https://github.com/Shraddhadeshmukh-123/webapp.git'
            }
        }
       stage('build') {
       steps {
        sh "mvn clean install" 
       }
       }
    }
}
